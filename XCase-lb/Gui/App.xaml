<Application x:Class="XCase.Gui.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   Startup="App_Startup"
    >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>         
                <ResourceDictionary Source="pack://application:,,,/View;component/ControlTemplates.xaml" />
                <ResourceDictionary Source="Styles/Shared.xaml" />
                <ResourceDictionary Source="Styles/ToolBar.xaml" />
				<ResourceDictionary Source="/ResourceLibrary;component/Images/Images.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
